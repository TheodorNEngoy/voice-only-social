{
  "name": "voxonly",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "concurrently \"next dev\" \"npm:worker\"",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "test": "vitest run --coverage",
    "test:watch": "vitest",
    "playwright": "playwright test",
    "typecheck": "tsc --noEmit",
    "migrate": "prisma migrate dev",
    "migrate:deploy": "prisma migrate deploy",
    "seed": "ts-node --project tsconfig.json scripts/seed.ts",
    "worker": "ts-node --project tsconfig.json worker/index.ts"
  },
  "dependencies": {
    "@opentelemetry/api": "^1.8.0",
