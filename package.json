    "@types/jsonwebtoken": "^9.0.5",
    "argon2": "^0.31.2",
    "bullmq": "^4.16.0",
    "clsx": "^2.1.0",
    "cookie": "^0.6.0",
    "crypto": "^1.0.1",
    "jsonwebtoken": "^9.0.2",
    "ioredis": "^5.4.1",
    "next": "14.2.3",
    "openapi-typescript": "^6.6.0",
    "pino": "^8.19.0",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-hook-form": "^7.51.4",
    "tailwind-merge": "^2.2.0",
